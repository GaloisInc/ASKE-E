let beta = 0.4
let gamma = 0.04

let s_initial = 997
let i_initial = 3
let r_initial = 0

var S = s_initial
var I = i_initial
var R = r_initial

let total_population = S + I + R

d/dt S = -(beta * S * I / total_population)              
d/dt I =  (beta * S * I / total_population) - (gamma * I)
d/dt R =                                       gamma * I 